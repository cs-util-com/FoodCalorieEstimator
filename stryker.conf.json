{
  "testRunner": "jest",
  "reporters": ["html", "clear-text", "json", "progress"],
  "mutate": ["src/**/*.js", "!src/**/*.test.js"],
  "coverageAnalysis": "perTest",
  "thresholds": { "high": 90, "low": 80, "break": 80 },
  "jsonReporter": {
    "fileName": "reports/mutation/mutation-report.json"
  }
}
